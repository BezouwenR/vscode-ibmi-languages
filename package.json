{
    "name": "ibmi-languages",
    "displayName": "IBMi Languages",
    "description": "Provide syntax highlighting for IBMi languages such as RPG, CL, DDS, and MI.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cl",
                "aliases": [
                    "cl",
                    "clle",
                    "clp",
                    "clp38"
                ],
                "extensions": [
                    ".CL",
                    ".CLLE",
                    ".CLP",
                    ".CLP38",
                    ".cl",
                    ".clle",
                    ".clp",
                    ".clp38"
                ],
                "configuration": "./cl.language-configuration.json"
            },
            {
                "id": "rpg",
                "aliases": [
                    "rpg",
                    "rpg/400",
                    "rpg38"
                ],
                "extensions": [
                    ".RPG",
                    ".rpg",
                    ".RPG38",
                    ".rpg38",
                    ".SQLRPG",
                    ".sqlrpg"
                ],
                "configuration": "./rpg.language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cl",
                "scopeName": "source.cl",
                "path": "./syntaxes/cl.tmLanguage.json"
            },
            {
                "language": "rpg",
                "scopeName": "source.rpg",
                "path": "./syntaxes/rpg.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^3.13.1"
    },
    "license": "MIT"
}
