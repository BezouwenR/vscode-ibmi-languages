{
    "name": "ibmi-languages",
    "displayName": "IBMi Languages",
    "description": "Provide syntax highlighting for IBMi languages such as RPG, CL, DDS, and MI.",
    "version": "0.2.0",
    "publisher": "barrettotte",
    "author": {
        "name": "Barrett Otte"
    },
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cl",
                "aliases": [
                    "CL",
                    "CLLE",
                    "CLP",
                    "CLP38"
                ],
                "extensions": [
                    ".CL",
                    ".CLLE",
                    ".CLP",
                    ".CLP38",
                    ".cl",
                    ".clle",
                    ".clp",
                    ".clp38"
                ],
                "configuration": "./configurations/cl.language-configuration.json"
            },
            {
                "id": "dds.dspf",
                "aliases": [
                    "DSPF"
                ],
                "extensions": [
                    ".DSPF",
                    ".DSPF38",
                    ".dspf",
                    ".dspf38"
                ],
                "configuration": "./configurations/dds.language-configuration.json"
            },
            {
                "id": "dds.lf",
                "aliases": [
                    "LF"
                ],
                "extensions": [
                    ".LF",
                    ".LF38",
                    ".lf",
                    ".lf38"
                ],
                "configuration": "./configurations/dds.language-configuration.json"
            },
            {
                "id": "dds.pf",
                "aliases": [
                    "DDS",
                    "PF"
                ],
                "extensions": [
                    ".DDS",
                    ".PF",
                    ".PF38",
                    ".dds",
                    ".pf",
                    ".pf38"
                ],
                "configuration": "./configurations/dds.language-configuration.json"
            },
            {
                "id": "rpg",
                "aliases": [
                    "RPG",
                    "RPG/400",
                    "RPG38"
                ],
                "extensions": [
                    ".RPG",
                    ".rpg",
                    ".RPG38",
                    ".rpg38",
                    ".SQLRPG",
                    ".sqlrpg"
                ],
                "configuration": "./configurations/rpg.language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cl",
                "scopeName": "source.cl",
                "path": "./syntaxes/cl.tmLanguage.json"
            },
            {
                "language": "dds.dspf",
                "scopeName": "source.dds.dspf",
                "path": "./syntaxes/dds.dspf.tmLanguage.json"
            },
            {
                "language": "dds.lf",
                "scopeName": "source.dds.lf",
                "path": "./syntaxes/dds.lf.tmLanguage.json"
            },
            {
                "language": "dds.pf",
                "scopeName": "source.dds.pf",
                "path": "./syntaxes/dds.pf.tmLanguage.json"
            },
            {
                "language": "rpg",
                "scopeName": "source.rpg",
                "path": "./syntaxes/rpg.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^3.13.1"
    },
    "keywords": [
        "RPG",
        "IBMi",
        "AS/400",
        "SQLRPG",
        "SQLRPGLE",
        "CL",
        "CONTROL LANGUAGE",
        "DDS",
        "DSPF",
        "MI",
        "RPGLE",
        "ISERIES"
    ],
    "license": "MIT",
    "repository": {
        "url": "https://github.com/barrettotte/vscode-ibmi-languages"
    }
}
