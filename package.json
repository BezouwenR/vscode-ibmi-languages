{
    "name": "ibmi-languages",
    "displayName": "IBMi Languages",
    "description": "Provide syntax highlighting for IBMi languages such as RPG, CL, DDS, and MI.",
    "version": "0.0.1",
    "publisher": "barrettotte",
    "author": {
        "name": "Barrett Otte"
    },
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cl",
                "aliases": [
                    "CL",
                    "CLLE",
                    "CLP",
                    "CLP38"
                ],
                "extensions": [
                    ".CL",
                    ".CLLE",
                    ".CLP",
                    ".CLP38",
                    ".cl",
                    ".clle",
                    ".clp",
                    ".clp38"
                ],
                "configuration": "./cl.language-configuration.json"
            },
            {
                "id": "dds",
                "aliases": [
                    "DDS",
                    "DSPF",
                    "LF",
                    "PF",
                    "PRTF"
                ],
                "extensions": [
                    ".DDS",
                    ".PF",
                    ".PF38",
                    ".LF",
                    ".LF38",
                    ".DSPF",
                    ".DSPF38",
                    ".PRTF",
                    ".PRTF38",
                    ".dds",
                    ".pf",
                    ".pf38",
                    ".lf",
                    ".lf38",
                    ".dspf",
                    ".dspf38",
                    ".prtf",
                    ".prtf38"
                ],
                "configuration": "./dds.language-configuration.json"
            },
            {
                "id": "rpg",
                "aliases": [
                    "RPG",
                    "RPG/400",
                    "RPG38"
                ],
                "extensions": [
                    ".RPG",
                    ".rpg",
                    ".RPG38",
                    ".rpg38",
                    ".SQLRPG",
                    ".sqlrpg"
                ],
                "configuration": "./rpg.language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cl",
                "scopeName": "source.cl",
                "path": "./syntaxes/cl.tmLanguage.json"
            },
            {
                "language": "dds",
                "scopeName": "source.dds",
                "path": "./syntaxes/dds.tmLanguage.json"
            },
            {
                "language": "rpg",
                "scopeName": "source.rpg",
                "path": "./syntaxes/rpg.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^3.13.1"
    },
    "keywords": [
        "RPG",
        "IBMi",
        "AS/400",
        "CL",
        "DDS",
        "DSPF",
        "MI",
        "RPGLE",
        "ISERIES"
    ],
    "license": "MIT"
}
